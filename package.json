{
  "name": "entretenimento",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:dev": "ng serve -c development",
    "start:emu": "ng serve -c dev-emu",
    "start:staging": "ng serve -c staging",
    "build": "ng build -c development",
    "build:dev": "ng build -c development",
    "build:emu": "ng build -c dev-emu",
    "build:staging": "ng build -c staging",
    "build:prod": "ng build -c production",
    "test": "jest -c jest.config.mjs",
    "functions:build": "npm --prefix functions run build",
    "emu:pre": "kill-port 4000 4400 4500 9099 8080 5001 8087 9000 9199 9150 || exit 0",
    "preemu:start": "npm run emu:pre && npm run functions:build",
    "emu:start": "npx firebase emulators:start --only auth,firestore,storage,functions,pubsub,database,ui --project entretenimento-sexual",
    "emu": "npm run emu:pre && npm run rules:build && npm run emu:start",
    "emu:noui:start": "npx firebase emulators:start --only auth,firestore,storage,functions,pubsub,database --project entretenimento-sexual",
    "emu:noui": "npm run emu:pre && npm run preemu:start && npm run emu:noui:start",
    "emulators:start:ui": "start http://127.0.0.1:4000",
    "rules:build": "node firestore-rules/tools-rules/build-firestore-rules.mjs",
    "rules:watch": "node firestore-rules/tools-rules/build-firestore-rules.mjs --watch",
    "emulators": "npm run emu:pre && npm run rules:build && firebase emulators:start",
    "deploy:rules": "npm run rules:build && firebase deploy --only firestore:rules"
  },
  "private": true,
  "dependencies": {
